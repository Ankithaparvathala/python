Add an item
a=(4,6,2,8,3,1)
print(a)
b=(9,)
c=a+b
print(c)
d=c[:5]+(15,20,25)+a[:5]
print(d)
e=list(d)
e.append(30)
e=tuple(e)
print(e)

#Length of a tuple
n=input()
a=tuple(n)
print(a)
print(len(a))

#Reverse a Tuple
n=input()
a=tuple(n)
print(a[::-1])

#Repeated element in a tuple
s=(2,4,5,6,2,3,4,4,7)
print(s)
n=int(input())
print(s.count(n))



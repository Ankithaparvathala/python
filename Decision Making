#Budget    
br=int(input())
vs=int(input())
ay=int(input())
ar=int(input())
leg=int(input())
tc=(br*350.34)+(vs*230.90)+(ay*190.55)+(ar*125.30)+(leg*180.90)
if tc>15000:
    print("no")
else:
    print("yes")
    
#Sece Dining
d=input()
r=int(input())
if d=='front' and r==1:
    print("Left Handed")
elif d=='front' and r==2:
    print("right Handed")
elif d=='rear' and r==1:
    print("right Handed")
else:
    print("Left Handed")
    
 #Intersection of a Circle
 x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
d=(((x1-x2)**2)+(y1-y2)**2)**(1/2)
if d<r1+r2:
    print("The circles overlap")
elif d==r1+r2:
    print("The circles are tangential")
else:
    print("The circles do not overlap")
    
# Microwave Oven
n=int(input("Enter the number of items:"))
h=float(input("Enter the single item heating time:"))
if n==2:
    t=(0.5*h)+h
    print("The recommended heating time is","%.2f"%t)
elif n==3:
    t=2*h
    print("The recommended heating time is","%.2f"%t)
else:
    print("Number of items is more")
        
    
    

    
    
